{
  "name": "wvus-dev-utilities",
  "version": "1.0.0",
  "description": "WVUS Dev Utilites",
  "main": "./dist/index.js",
  "keywords": [
    "Git",
    "CLI"
  ],
  "author": "Scott Rees & Adam Murray",
  "license": "MIT",
  "dependencies": {
    "chalk": "^1.1.3",
    "clear": "0.0.1",
    "clui": "^0.3.1",
    "figlet": "^1.1.2",
    "github": "^2.1.0",
    "inquirer": "^1.1.0",
    "lodash": "^4.13.1",
    "minimist": "^1.2.0",
    "preferences": "^0.2.1",
    "simple-git": "^1.40.0",
    "touch": "^1.0.0"
  },
  "bin": {
    "wvus-dev-utilities": "./dist/index.js"
  },
  "scripts": {
    "build": "babel src --presets babel-preset-es2015 --out-dir dist",
    "watch": "npm-watch"
  },
  "watch": {
    "build": {
      "patterns": [
        "./src"
      ],
      "extensions": "js",
      "quiet": false
    }
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "npm-watch": "^0.1.7"
  }
}
